<prism:PrismUnityApplication
    xmlns:prism="using:Prism.Unity.Windows"
    x:Class="SCommerce.Main.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <prism:PrismUnityApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Light" Source="Themes/LightModeColors.xaml" />
                <ResourceDictionary x:Key="Dark" Source="Themes/DarkModeColors.xaml" />
                <ResourceDictionary x:Key="HighContrast" Source="Themes/HighContrastColors.xaml" />
            </ResourceDictionary.ThemeDictionaries>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/Generic.xaml" />
                <ResourceDictionary Source="Controls/CustomRoundedButtonControl/CustomRoundedButtonControl.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </prism:PrismUnityApplication.Resources>
    
</prism:PrismUnityApplication>
